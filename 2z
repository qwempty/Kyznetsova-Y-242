a = int(input('Введите целое число'))
b = int(input('Введите целое число'))
c = int(input('Введите целое число'))
if a>3:
    print(a, 'Не входит в отрезок')
elif a<1:
    print(a, 'Не входит в отрезок')
else:
    print(a, 'Входит в отрезок')
if b>3:
    print(b, 'Не входит в отрезок')
elif b<1:
    print(b, 'Не входит в отрезок')
else:
    print(b, 'Входит в отрезок')
if c>3:
    print(c, 'Не входит в отрезок')
elif c<1:
    print(c, 'Не входит в отрезок')
else:
    print(c, 'Входит в отрезок')
